{
  "name": "phonypony",
  "version": "1.1.0",
  "description": "A TypeScript tool for generating p(h)ony data from TypeScript interfaces",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "bin": {
    "phonypony": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix"
  },
  "keywords": [
    "typescript",
    "mock",
    "data",
    "generator",
    "cli",
    "faker",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
